{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "expires_at": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string",
          "oneOf": [
            {
              "format": "date"
            },
            {
              "format": "date-time"
            },
            {
              "pattern": "^$"
            }
          ]
        }
      ]
    },
    "member_role_id": {
      "type": [
        "number",
        "null"
      ]
    }
  },
  "additionalProperties": true
}
