- support_group_level_mr_setting = @project.licensed_feature_available?(:group_level_merge_checks_setting)

- if !support_group_level_mr_setting
  = render_ce 'projects/settings/merge_requests/merge_request_pipelines_and_threads_options', form: form, project: project
- else
  .js-merge-request-merge-checks{ data: merge_checks(@project) }
