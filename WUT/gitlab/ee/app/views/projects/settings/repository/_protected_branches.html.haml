- if @project.feature_available?(:protected_refs_for_users, current_user)
  .js-protected-refs-for-users
    - if can?(current_user, :admin_protected_branch, @project)
      = render "protected_branches/ee/index", protected_branch_entity: protected_branch_entity
    - if can?(current_user, :admin_project, @project)
      = render "projects/protected_tags/ee/index"
- else
  - if can?(current_user, :admin_protected_branch, @project)
    = render "protected_branches/index", protected_branch_entity: protected_branch_entity
  - if can?(current_user, :admin_project, @project)
    = render "projects/protected_tags/index"
