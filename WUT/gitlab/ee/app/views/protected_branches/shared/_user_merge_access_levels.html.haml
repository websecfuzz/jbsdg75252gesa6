- user_merge_access_levels = protected_branch.merge_access_levels.for_user
- if user_merge_access_levels.any?
  %p.small
    = n_('The following user can also merge into this branch: %{branch}', 'The following users can also merge into this branch: %{branch}', user_merge_access_levels.size) % { branch: user_merge_access_levels.map(&:humanize).to_sentence }
