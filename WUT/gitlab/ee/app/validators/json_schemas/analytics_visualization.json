{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/AnalyticsVisualization",
  "definitions": {
    "AnalyticsVisualization": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "version": {
          "const": 1
        },
        "type": {
          "enum": [
            "AreaChart",
            "LineChart",
            "ColumnChart",
            "DataTable",
            "SingleStat",
            "DORAChart",
            "UsageOverview",
            "DoraPerformersScore",
            "DoraProjectsComparison",
            "AiImpactTable",
            "ContributionsByUserTable",
            "ContributionsPushesChart",
            "ContributionsIssuesChart",
            "ContributionsMergeRequestsChart",
            "NamespaceMetadata",
            "MergeRequestsThroughputTable"
          ]
        },
        "slug": {
          "type": "string"
        },
        "options": {
          "$ref": "#/definitions/Options"
        },
        "data": {
          "$ref": "#/definitions/Data"
        },
        "errors": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "data",
        "options",
        "type"
      ],
      "title": "AnalyticsVisualization"
    },
    "Data": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "enum": [
            "cube_analytics",
            "value_stream",
            "usage_overview",
            "usage_count",
            "dora_metrics",
            "dora_metrics_by_project",
            "ai_impact_over_time",
            "contributions",
            "namespace_metadata",
            "merge_requests",
            "merge_request_counts",
            "mean_time_to_merge"
          ]
        },
        "query": {
          "type": "object"
        }
      },
      "required": [
        "query",
        "type"
      ],
      "title": "Data"
    },
    "Options": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "xAxis": {
          "$ref": "#/definitions/XAxis"
        },
        "yAxis": {
          "$ref": "#/definitions/YAxis"
        },
        "chartTooltip": {
          "$ref": "#/definitions/ChartTooltip"
        },
        "links": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Link"
          }
        },
        "decimalPlaces": {
          "type": "integer"
        },
        "unit": {
          "type": "string"
        },
        "metaText": {
          "type": "string"
        },
        "metaIcon": {
          "type": "string"
        },
        "titleIcon": {
          "type": "string"
        },
        "includeLegendAvgMax": {
          "type": "boolean"
        },
        "fields": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "component": {
                "enum": [
                  "AssigneeAvatars",
                  "DiffLineChanges",
                  "MergeRequestLink"
                ]
              }
            }
          }
        }
      },
      "title": "Options"
    },
    "XAxis": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "min": {
          "type": "integer"
        },
        "max": {
          "type": "integer"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "title": "XAxis"
    },
    "YAxis": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "min": {
          "type": "integer"
        },
        "max": {
          "type": "integer"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "title": "YAxis"
    },
    "Link": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "text": {
          "type": "string"
        },
        "href": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          ]
        }
      },
      "required": [
        "text",
        "href"
      ],
      "title": "Link"
    },
    "ChartTooltip": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "valueUnit": {
          "type": {
            "enum": [
              "DAYS",
              "PER_DAY",
              "PERCENT",
              "TIME_INTERVAL"
            ]
          }
        }
      },
      "title": "ChartTooltip"
    }
  }
}
